{
  "name": "Dinorss",
  "version": "0.1.2",
  "description": "Dinasaur RSS",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev:win": "concurrently \"yarn run autobuild\" \"set ELECTRON_RELOAD=yes && electron .\"",
    "dev:mac": "concurrently \"yarn run autobuild\" \"ELECTRON_RELOAD=yes electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "storybook": "start-storybook -s ./public -p 12315 --ci & electron .storybook/electron.index.js"
  },
  "repository": "https://github.com/richshaw2015/dino-rss-electron",
  "keywords": [
    "Dinasaur",
    "RSS",
    "reader",
    "news"
  ],
  "author": "Richshaw",
  "license": "CC0-1.0",
  "build": {
    "appId": "org.dinorss",
    "productName": "Dinasaur RSS",
    "artifactName": "${productName}.${version}.${ext}",
    "files": [
      "!public/*.map"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon/logo.png"
    },
    "mac": {
      "icon": "public/icon/logo.icns",
      "type": "development",
      "category": "public.app-category.news",
      "target": [
        "dmg",
        "mas"
      ],
      "extendInfo": {}
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {},
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/svelte": "6.0.21",
    "concurrently": "^5.3.0",
    "electron": "^10.1.6",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-reload": "^1.5.0",
    "rollup": "^2.27.1",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.25.1",
    "svelte-loader": "^2.13.6",
    "telnet-client": "^1.4.6"
  },
  "dependencies": {
    "autolinker": "^3.14.2",
    "materialize-css": "^1.0.0",
    "md5": "^2.3.0",
    "mousetrap": "^1.6.5",
    "prismjs": "^1.22.0",
    "uuid": "^8.3.1"
  },
  "optionalDependencies": {
    "@podlove/web-player": "^5.2.3",
    "fsevents": "^2.2.1",
    "highlight.js": "^10.3.1",
    "qrcodejs": "^1.0.0"
  }
}
